    "dependencies": {
        "compression": "^1.7.4",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "module-alias": "^2.2.2",
        "sequelize": "^6.5.1",
        "sqlite3": "^5.0.2"
    },
    "devDependencies": {
        "@types/compression": "^1.7.0",
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.20",
        "@types/module-alias": "^2.0.0",
        "@types/node": "^14.14.27",
        "@types/supertest": "^2.0.10",
        "jest": "^26.6.3",
        "sequelize-cli": "^6.2.0",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.3",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.1",
        "tsc": "^1.20150623.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.1.3"
    },
    "scripts": {
        "build": "tsc --build build.json",
        "start": "npm run build && node dist/app.js",
        "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/app.ts",
        "pretest": "NODE_ENV=test sequelize db:migrate:undo:all > \"/dev/null\" 2>&1 && sequelize db:migrate > \"/dev/null\" 2>&1",
        "test": "jest",
        "posttest": "NODE_ENV=test sequelize db:migrate:undo:all > \"/dev/null\" 2>&1"
    },
    "_moduleAliases": {
        "@": "dist",
        "@domain": "dist/domain",
        "@data": "dist/data",
        "@infrastructure": "dist/infrastructure",
        "@http": "dist/infrastructure/http"
    }
}
